{
  "expo": {
    "name": "RebellionAgainstTheKing",
    "slug": "k-card",
    "version": "1.0.0",
    "orientation": "landscape",
    "scheme": "rebellionagainsttheking",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joshuadascolisorganization.kcard",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.joshuadascolisorganization.kcard"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dc3ef01c-c26e-465d-b532-f2b78c15b152"
      }
    },
    "owner": "joshuadascolis-organization"
  }
}
